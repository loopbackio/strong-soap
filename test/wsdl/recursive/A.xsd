<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://www.Dummy.com/Common/Types"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns="http://www.Dummy.com/Common/Types"
>
    <xs:import namespace="http://www.Dummy.com/Name/Types"
               schemaLocation="B.xsd"/>
    <xs:complexType name="ContactMedium">
        <xs:annotation>
            <xs:documentation>Defines the method of contact to reach a party (in
                their specified role)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="validFor" type="xs:date" minOccurs="0"
                        maxOccurs="1"/>
            <xs:element name="preference" type="xs:integer" minOccurs="0"
                        maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Indicates that this contact medium is the
                        preferred (if multiple are present). Lower integer takes
                        precedence, "equals are equals" so the value does not
                        need to be
                        unique.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Address" type="xs:string" minOccurs="0"
                        maxOccurs="1"/>
            <xs:element name="Email" type="xs:string" minOccurs="0"
                        maxOccurs="1"/>
            <xs:element name="Telephone" type="xs:string" minOccurs="0"
                        maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
