    "lodash": "^4.13.1",
    "node-uuid": "^1.4.7",
    "optional": "^0.1.3",
    "request": "^2.72.0",
    "sax": "^1.2",
    "selectn": "^1.0.20",
    "strip-bom": "^3.0.0",
