{
  "name": "strong-soap",
  "version": "3.2.1",
  "description": "A minimal node SOAP client",
  "engines": {
    "node": ">=8.11.1"
  },
  "dependencies": {
    "compress": "^0.99.0",
    "debug": "^4.1.1",
