{
  "name": "strong-soap",
  "version": "1.4.2",
  "description": "A minimal node SOAP client",
  "engines": {
    "node": ">=4.0"
  },
  "dependencies": {
    "compress": "^0.99.0",
    "debug": "^2.2.0",
    "lodash": "^4.13.1",
    "uuid": "^3.0.0",
    "optional": "^0.1.3",
    "path": "^0.12.7",
    "request": "^2.72.0",
    "sax": "^1.2",
    "selectn": "^1.0.20",
    "strong-globalize": "^2.8.0",
